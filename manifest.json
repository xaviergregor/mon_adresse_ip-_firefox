{
    "manifest_version": 2,
    "name": "Mon IP Publique",
    "version": "1.46",
    "description": "Affiche mon adresse IP publique",
    "permissions": ["https://api.ipify.org/"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  
